class <%= @users.first.module_name %>::<%= @users.first.class_name %> {
<% @users.each do |u| %>
  user {'<%= u.name %>':
    ensure     => present,
    managehome => true,
    <% if u.provider %>
    provider   => '<%= u.provider %>',
    <% end %>
    <% if u.userid %>
    uid        => '<%= u.userid %>',
    <% end %>
    <% if u.groupid %>
    gid        => '<%= u.groupid %>',
    <% end %>
    <% if u.homedir %>
    home       => '<%= u.homedir %>',
    <% end %>
    <% if u.shell %>
    shell      => '<%= u.shell %>',
    <% end %>
  }
<% end %>
}
